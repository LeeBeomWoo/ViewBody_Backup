<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->

    <style name="ViewBody" parent="Theme.AppCompat.Light.NoActionBar">>
        <item name="background">@drawable/background</item>
        <item name="colorPrimary">@color/viewbodyappbarcolor</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/viewstatusbarcolor</item>
        <item name="android:navigationBarColor">@color/viewbodyhombar</item>
    </style>
    <style name="ViewBody.main" parent="Theme.AppCompat.Light">>
        <item name="background">@drawable/background</item>
        <item name="colorPrimary">@color/viewbodyappbarcolor_main</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/viewstatusbarcolor_main</item>
        <item name="android:navigationBarColor">@color/viewbodyhombar_main</item>
        <item name="android:colorForeground" tools:ignore="PrivateResource">@color/foreground_material_light</item>
        <item name="android:colorBackground">@color/view_body_background_light</item>
    </style>

</resources>
